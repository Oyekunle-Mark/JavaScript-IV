{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "indent": 0,
        "comma-dangle": 0,
        "no-console": 0,
        "quotes": 0,
        "class-methods-use-this": 0,
        "no-param-reassign": 0,
        "no-unneeded-ternary": 0,
        "arrow-body-style": 0
    }
}